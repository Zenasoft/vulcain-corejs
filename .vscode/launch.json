{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/samples/index.ts",
			"stopOnEntry": false,
			"preLaunchTask": "compile-ts",
			"args": [],
			"smartStep": false,
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
			],
			"env": {
				"VULCAIN_ENV": "vulcain",
				"VULCAIN_SERVICE_NAME": "core",
				"VULCAIN_SERVICE_VERSION": "1.0",
				"VULCAIN_DOMAIN": "test",
				"NODE_ENV": "development"
			},
			"console": "internalConsole",
			"sourceMaps": true,
			"outFiles": ["${workspaceRoot}/dist/**/*.js"]
		},
        {
            "name": "Test",
            "type": "node",
			"request": "launch",
			"smartStep": false,
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "preLaunchTask": "compile-test",
            "args": ["${workspaceRoot}/dist-test/**/*.js"],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
            ],
            "env": {
				"VULCAIN_ENV": "vulcain",
				"VULCAIN_SERVICE_NAME": "core",
				"VULCAIN_SERVICE_VERSION": "1.0",
				"VULCAIN_DOMAIN": "test",
				"NODE_ENV": "development"
            },
			"console": "internalConsole",
            "sourceMaps": true,
            "outFiles": ["${workspaceRoot}/dist-test/**/*.js", "${workspaceRoot}/dist/**/*.js"]
        },
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		}
	]
}
